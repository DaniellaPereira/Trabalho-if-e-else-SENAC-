x = int (input("Digite um numero correspondente ao dia da semana: "))

if (x == 1) :
    print("Domingo")
    
elif (x == 2) :
    print("Segunda-feira")
    
elif (x == 3) :
    print("Terça-feira")
    
elif (x == 4) :
    print("Quarta-feira")
    
elif (x == 5) :
    print("Quinta-feira")
    
elif (x == 6) :
    print("Sexta-feira")
    
elif (x == 7) :
    print("Sábado")
    
else :
    print("Valor inválido")
